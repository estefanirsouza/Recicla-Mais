								"exec": [
									"// Extract 'token' from response and set as collection variable if present\r",
									"pm.test(\"Extract and set token as collection variable\", function () {\r",
									"    if (pm.response.code === 200) {\r",
									"        let jsonData;\r",
									"        try {\r",
									"            jsonData = pm.response.json();\r",
									"        } catch (e) {\r",
									"            // Not a JSON response\r",
									"            return;\r",
									"        }\r",
									"        if (jsonData && jsonData.token) {\r",
									"            pm.collectionVariables.set(\"token\", jsonData.token);\r",
									"            pm.expect(jsonData.token, \"Token should be a non-empty string\").to.be.a(\"string\").and.not.empty;\r",
									"        }\r",
